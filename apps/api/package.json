{
  "name": "@canary/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts --format esm --dts",
    "start": "node dist/server.js",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@canary/shared": "workspace:*",
    "@fastify/cookie": "^11.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/multipart": "^9.3.0",
    "@fastify/redis": "^7.0.1",
    "@fastify/session": "^11.0.1",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.1",
    "@sendgrid/mail": "^8.1.4",
    "@types/form-data": "^2.5.2",
    "argon2": "^0.41.1",
    "bullmq": "^5.30.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.38.3",
    "fastify": "^5.2.0",
    "form-data": "^4.0.5",
    "handlebars": "^4.7.8",
    "ioredis": "^5.4.2",
    "mailgun.js": "^10.2.3",
    "nanoid": "^5.0.9",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.1",
    "postmark": "^4.0.5",
    "resend": "^4.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@aws-sdk/client-ses": "^3.712.0",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.30.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
